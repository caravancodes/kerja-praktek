var elArrowImgUp                = 'data:image/svg+xml;utf8;base64,';
var elArrowImgDown              = 'data:image/svg+xml;utf8;base64,';

var propsImageReset = 'border: 0; position: inherit; max-width: inherit; left: inherit; right: inherit; top: inherit; bottom: inherit; margin-bottom:inherit;';

var cccCurrentTheme = {
    General: {
      background: '#FFF',
		  borderWidth: '1px',
	    borderColor: '#e0e6ef'
		},
		Tabs: {
		  background: '#F5F5F5',
		  color: '#000',			
		  activeBackground: '#FFF',
		  activeColor: '#000',
		},			
		Row: {
		  color: '#000',
		  borderColor: '#E0E6EF'
		},			
		Trend: {
		  colorUp: '#3d9400',
		  colorDown: '#A11B0A',
		  colorUnchanged: '#2C4C76'				
		},			
		Conversion: {
		  background: 'transparent',
		  lineHeight: '20px',
		  color: '#000',
		}
};


if (typeof cccTheme !== 'undefined') {
  for(var key in cccCurrentTheme) {
    var group = cccCurrentTheme[key];
    for(var prop in group) {
      if(!group.hasOwnProperty(prop)) continue;
      if(cccTheme.hasOwnProperty(key) && cccTheme[key].hasOwnProperty(prop)) {
        cccCurrentTheme[key][prop] = cccTheme[key][prop];
      }
    }
  }
}

if (typeof cccThemeV1Multi !== 'undefined') {
  for(var key in cccCurrentTheme) {
    var group = cccCurrentTheme[key];
    for(var prop in group) {
      if(!group.hasOwnProperty(prop)) continue;
      if(cccThemeV1Multi.hasOwnProperty(key) && cccThemeV1Multi[key].hasOwnProperty(prop)) {
        cccCurrentTheme[key][prop] = cccThemeV1Multi[key][prop];
      }
    }
  }
}

