var tabOnClick = function(sym) {
  var elsTabs = document.querySelectorAll('[class^="ccc-tab-header-"]');
  
  for (var i = 0; i < elsTabs.length; ++i) {
      var el = elsTabs[i];
    
      if('ccc-tab-header-' + sym == el.className) {
        el.style.background = cccCurrentTheme.Tabs.activeBackground;
        el.style.color = cccCurrentTheme.Tabs.activeColor;
      }
      else {
        el.style.background = cccCurrentTheme.Tabs.background;
      }
  }      
  
  var elsContents = document.querySelectorAll('[class^="ccc-tab-content-"]');
  for (var i = 0; i < elsContents.length; ++i) {
      var el = elsContents[i];
   
      if('ccc-tab-content-' + sym == el.className) {
        el.style.display = 'block';
      }
      else {
        el.style.display = 'none';
      }
  }
  
  return false;
  
}  


  
  
  var tab                   = document.createElement('a');
  tab.className             = 'ccc-tab-header-USD';    
  tab.style.display         = 'block';
  tab.rel                   = 'nofollow';
  tab.style.background      = cccCurrentTheme.Tabs.activeBackground;
  
  tab.style.color           = cccCurrentTheme.Tabs.activeColor + '!important';
  
  tab.style.color           = cccCurrentTheme.Tabs.color;

  tab.style.textDecoration  = 'none';    
  tab.style.lineHeight      = '37px';
  tab.style.float           = 'left';
  tab.style.width           = '25%';
  tab.style.textAlign       = 'center';
  tab.style.fontWeight      = 'bold';
  tab.href                  = '';
  tab.innerHTML             = 'USD';
  tab.onclick               = function() {
    tabOnClick('USD');
    return false;
  }; 
  menu.appendChild(tab);
  
  
